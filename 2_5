/*2_5.讓螢幕會做秀-來個表格(20.0)*/
#include <stdio.h>
int main(){
     printf(  "1\t1\t1\n" );
     printf(  "2\t4\t8\n" );
     printf(  "3\t9\t27\n" );
     printf(  "4\t16\t64\n" );
     printf(  "5\t25\t125\n" );
     printf(  "6\t36\t216\n" );
     printf(  "7\t49\t343\n" );
     printf(  "8\t64\t512\n" );
     printf(  "9\t81\t729\n" );
     printf(  "10\t100\t1000\n" );
	return 0;
}
