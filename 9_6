/*9_6.else魔法-是否為3的倍數-2?(10.0)*/
#include <stdio.h>
int main(){
    int a,b,c,d;
    scanf("%d",&a);
    b=a/100;
    c=(a-(a/100)*100)/10;
    d=a-(a/10)*10;
    if(b%3==0)
        printf("百位數%d是3的倍數\n",b);
    else
        printf("百位數%d不是3的倍數\n",b);
    
    if(c%3==0)
        printf("十位數%d是3的倍數\n",c);
    else
        printf("十位數%d不是3的倍數\n",c);
    
    if(d%3==0)
        printf("個位數%d是3的倍數\n",d);
    else
        printf("個位數%d不是3的倍數\n",d);
	return 0;
}
